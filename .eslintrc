{
  "env": {
    "es6": true
  },
  "extends": [
    "plugin:vue/recommended",
    "plugin:vue/base",
    "plugin:vuetify/base",
    "eslint:recommended"
  ],
  "globals": {
  },
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "plugins": [
  ],
  "rules": {
    "no-extra-parens": "error",
    "semi": ["error", "never"],
    "quotes": ["error", "single"],
    "quote-props": ["error", "as-needed"],
    "space-before-function-paren": "error",
    "keyword-spacing": "error",
    "spaced-comment": ["error", "always"]
  }
}